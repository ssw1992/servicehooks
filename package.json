{
  "name": "servicehooks",
  "private": false,
  "version": "0.0.8",
  "files": [
    "lib"
  ],
  "main": "./lib/index.umd.js",
  "module": "./lib/index.es.js",
  "unpkg": "./lib/index.min.js",
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js"
    }
  },
  "types": "./types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "vue-tsc --noEmit && vite build",
    "build-npm": "vite build --mode npm && tsup",
    "publish-main": "npm run build-npm && npm publish --access public --registry https://registry.npmjs.org",
    "publish-beta": "npm run publish-main --tag beta",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "tsup": "^8.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.25"
  }
}
